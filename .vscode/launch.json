            "script": "${workspaceRoot}\\DelegationConfig.ps1",
            "cwd": "${cwd}",
            "args": ["-adddelegation -computerOU \"OU=BG-Computer,DC=bloedgelaber,DC=de\"-ADUserOrGroup \"Tier-1-BG-Server-Admins\" -Domain \"bloedgelaber.de\"" ]
