            "cwd": "${cwd}",
            "args": ["-adddelegation -computerOU \"OU=BG-Servers,DC=bloedgelaber,DC=de\"-ADUserOrGroup \"Tier-1-BG-Server-Admins\" -Domain \"bloedgelaber.de\"" ]
        },
        {
            "name": "PowerShell Delegation script Add AA",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceRoot}\\DelegationConfig.ps1",
            "cwd": "${cwd}",
            "args": ["-adddelegation -computerOU \"OU=BG-Servers,DC=bloedgelaber,DC=de\"-ADUserOrGroup \"Kili\" -Domain \"bloedgelaber.de\"" ]
        },
        {
            "name": "PowerShell: DelegationConfig.ps1 ShowDelegation",
